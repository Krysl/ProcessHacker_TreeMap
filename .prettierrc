# yaml-language-server: $schema=http://json.schemastore.org/prettierrc
# https://prettier.io/docs/en/options.html

printWidth: 120
tabWidth: 2
semi: true
singleQuote: true
trailingComma: 'es5'
endOfLine: lf
overrides:
    - files: '*.json'
      options:
          tabWidth: 2
          printWidth: 80
